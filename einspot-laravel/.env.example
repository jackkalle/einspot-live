APP_NAME="Einspot Solutions"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=https://yourdomain.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4 # Not relevant for shared hosting

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack # Consider 'daily' or 'stderr' for shared hosting if single is problematic
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error # Should be 'error' or 'warning' in production

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=your_db_name
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN= # Set to your domain in production if using subdomains
SESSION_SECURE_COOKIE=true # Recommended for production (HTTPS)

BROADCAST_CONNECTION=log # Or 'pusher' if using real-time features
FILESYSTEM_DISK=public # Default disk for public files
QUEUE_CONNECTION=sync # 'database' if background jobs are needed and supported

CACHE_STORE=file # 'database' or 'redis' if available and preferred
CACHE_PREFIX=einspot_cache

# MEMCACHED_HOST=127.0.0.1 # Only if using memcached

# REDIS_CLIENT=phpredis # Only if using Redis
# REDIS_HOST=127.0.0.1
# REDIS_PASSWORD=null
# REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=your_smtp_host
MAIL_PORT=587 # Or 465, 2525, etc.
MAIL_USERNAME=your_smtp_username
MAIL_PASSWORD=your_smtp_password
MAIL_ENCRYPTION=tls # Or 'ssl', null
MAIL_FROM_ADDRESS="noreply@yourdomain.com"
MAIL_FROM_NAME="${APP_NAME}"
ADMIN_NOTIFICATION_EMAIL=admin@yourdomain.com # For admin emails

# AWS_ACCESS_KEY_ID= # Only if using AWS S3 for storage directly
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1
# AWS_BUCKET=
# AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
# VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}" # Only if using Pusher
# VITE_PUSHER_HOST="${PUSHER_HOST}"
# VITE_PUSHER_PORT="${PUSHER_PORT}"
# VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
# VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
